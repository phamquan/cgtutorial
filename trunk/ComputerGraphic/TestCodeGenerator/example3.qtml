<?xml version="1.0" ?>
<watermelon program='Computer Graphic Tutorial'>

<environment>
	<camera xpos=1 ypos=1 zpos=1 xlook=0 ylook=0 zlook=0 xup=0 yup=1 zup=0 />
	
	<projection type='ortho' left=-5 right=5 bottom=-3 top=3 near=-10 far=100 />

</environment>


<object>
	<rotate x=0 y=0 z=1 angle=-90>
		<line x1=0 y1=0 z1=0 x2=1 y2=0 z2=0 />
	</rotate>
	<rotate x=0 y=0 z=1 angle=45>
		<rectangle top=1 bottom=0 left=0 right=1 />
	</rotate>
</object>

</watermelon>
